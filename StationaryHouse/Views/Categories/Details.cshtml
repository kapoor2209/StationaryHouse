@model StationaryHouse.Models.Category

@{
    ViewData["Title"] = "Details";
}

<h1  class="heading text-center">Category Details</h1>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.CategoryName)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CategoryName)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.CategoryID">Edit</a> |
    <a asp-action="Index">All Categories</a>
</div>
